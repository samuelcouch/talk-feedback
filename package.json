{
  "name": "talk-feedback",
  "private": true,
  "version": "0.0.1",
  "description": "Get categoric feedback on your talks",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --exec \"babel-node src --presets es2015,stage-0\"",
    "now-build": "./node_modules/babel-cli/bin/babel.js src -s -D -d dist --presets es2015,stage-0",
    "now-start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/.git"
  },
  "author": "Sam Couch <sam@couch.rocks>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-browserify": "^1.0.2",
    "express-react-views": "^0.10.2",
    "jquery": "^3.2.1",
    "micro": "^7.3.2",
    "morgan": "^1.8.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "sqlite3": "^3.1.8",
    "watson-developer-cloud": "^2.29.0",
    "watson-react-components": "^0.6.7"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/"
  },
  "homepage": "https://github.com/"
}
